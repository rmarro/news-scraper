<div class="container">
    {{#each article}}
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <a target="_blank" href={{this.link}}><h3 class="panel-title">{{this.title}}</h3></a>
                </div>
                <div class="panel-body">
                    {{this.summary}}
                </div>
                <button type="button" class="btn btn-default unsave-article" data-id={{id}}>Unsave Article</button>
            </div>
        </div>
    </div>
    {{/each}}
</div>